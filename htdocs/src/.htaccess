Options -Indexes
DirectoryIndex itemlist.php

<ifModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^([^/]+)/([^/]+)/$ item.php?owner=$1&repository=$2 [L]
  RewriteRule ^([^/]+)/$ itemlist.php?owner=$1 [L]
</ifModule>
